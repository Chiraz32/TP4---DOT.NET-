    @{
        @model IEnumerable<String>

    }

<h1>List of courses </h1>
<br />
    <div >

        <ul>
            @{
            foreach (String s in Model)
            {
                <li href="Course/getcourse/@s">
                   
                        @s
                    
                </li>
            }
            }
        </ul>
    </div>

