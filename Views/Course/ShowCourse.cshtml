@{
    @model IEnumerable<Student>

}
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

@{

    if (Model.Count() <= 0)
    {
        <h2 class="alert alert-warning" role="alert">There is no Student studying this course please verify your infos </h2>
    }
    else

    {
        <h1>@ViewBag.Id</h1>
        <div>

            @foreach (Student s in Model)
            {
                <div>
                    @s.First_name  
                    <span>  @s.Last_name </span>
                  
                </div> 

            }

        </div>
    }

}
<br />
<br />
<br />
<br />

<button type="button" class="btn btn-info"><a style="color : black " asp-controller="Course" asp-action="AllCourses"> GO BACK TO ALL COURSES</a></button>